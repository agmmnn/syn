{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "synonym-cli-raycast",
  "title": "Synonym CLI",
  "description": "Look up synonyms, antonyms, and related words using Thesaurus.com and Datamuse API.",
  "icon": "command-icon.png",
  "author": "agmmnn",
  "categories": [
    "Productivity",
    "Writing"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "thesaurus",
      "title": "Thesaurus",
      "subtitle": "Search Synonyms",
      "description": "Get synonyms and antonyms from Thesaurus.com",
      "mode": "view"
    },
    {
      "name": "datamuse",
      "title": "Datamuse",
      "subtitle": "Explore Words",
      "description": "Explore words using Datamuse API",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.16.3",
    "cheerio": "^1.0.0-rc.12",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "raycast build",
    "dev": "raycast develop",
    "fix-lint": "raycast lint --fix",
    "lint": "raycast lint"
  }
}
